#7. Calculate the following:
        1  2           10   20
 2/7(   2  4    -      30   40   )
        7  6           50   60




> A <- matrix(c(1, 2, 2, 4, 7, 6), nrow = 3, byrow = TRUE)
> B <- matrix(c(10, 20, 30, 40, 50, 60), nrow = 3, byrow = TRUE)
> 
> result <- A - B
> print(result)
     [,1] [,2]
[1,]   -9  -18
[2,]  -28  -36
[3,]  -43  -54
> 
> result<-2/7*(result)
> result
           [,1]       [,2]
[1,]  -2.571429  -5.142857
[2,]  -8.000000 -10.285714
[3,] -12.285714 -15.428571
  
